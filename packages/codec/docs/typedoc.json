{
  "name": "Truffle Decoding and Encoding",
  "mode": "modules",
  "module": "commonjs",
  "theme": "default",
  "includeDeclarations": false,
  "ignoreCompilerErrors": true,
  "preserveConstEnums": true,
  "excludeExternals": true,
  "categoryOrder": [
    "Data",
    "ABI data location",
    "Solidity data location",
    "Common data location",
    "*",
    "Other"
  ],
  "exclude": [
    "**/test/*"
  ],
  "target": "ES6"
}
