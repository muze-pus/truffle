{
  "name": "@truffle/dashboard",
  "version": "0.1.0",
  "main": "build/server/index.js",
  "types": "build/server/index.d.ts",
  "files": [
    "build"
  ],
  "dependencies": {
    "@truffle/dashboard-message-bus": "^0.1.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "ethers": "^5.4.7",
    "express": "^4.17.1",
    "react-json-view": "^1.21.3"
  },
  "scripts": {
    "start:react": "DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true craco start",
    "start:prebuilt": "node build/server/start-dashboard.js 5000 localhost",
    "start:dev-backend": "ts-node --project tsconfig.server.json server/start-dashboard.ts 5000 localhost",
    "start": "concurrently --kill-others \"yarn start:dev-backend\" \"NODE_ENV=\"development\" yarn start:react\"",
    "build:react": "DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true craco build",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "yarn build:react && yarn build:server",
    "prepare": "yarn build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cors": "^2.8.12",
    "@types/ethereum-protocol": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/web3": "^1.0.20",
    "autoprefixer": "^9",
    "concurrently": "^6.2.0",
    "delay": "^5.0.0",
    "ethereum-protocol": "^1.0.1",
    "ethers": "^5.1.4",
    "postcss": "^7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-node": "^10.1.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}
